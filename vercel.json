{
  "buildCommand": "npm run build",
  "framework": "other",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/",
      "destination": "/app/index.html"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/app/index.html"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/.*"
    }
  ]
}
